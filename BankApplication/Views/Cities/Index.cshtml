@model IEnumerable<BankApplication.City>

<h1>Index</h1>

<a class="nav-link text-dark" href="@Url.Action("GroupbyCity", "Cities")">Grouped by cities</a>

<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CityCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CityName)
            </td>
        </tr>
    }
</table>
