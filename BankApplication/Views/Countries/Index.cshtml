@model IEnumerable<BankApplication.Country>

<h1>Index</h1>

<a class="nav-link text-dark" href="@Url.Action("GroupbyCounrty", "Countries")">Grouped by countries</a>

<table class="table">

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CountryCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CountryName)
            </td>
        </tr>
    }
</table>